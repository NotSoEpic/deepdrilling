{
  "schemaVersion": 1,
  "id": "deepdrilling",
  "version": "${version}",
  "name": "Deep Drilling",
  "description": "An addon for Create, adding powerful drills that extract ores from deep within the earth",
  "authors": [
    "beeisyou"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/deepdrilling",
    "sources": "https://github.com/NotSoEpic/deepdrilling"
  },
  "license": "MIT",
  "icon": "assets/deepdrilling/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.deepdrilling.fabric.DrillModFabric"
    ],
    "fabric-datagen": [
      "com.deepdrilling.fabric.DrillModDatagen"
    ],
    "client": [
      "com.deepdrilling.fabric.DrillModClientFabric"
    ],
    "jei_mod_plugin": [
      "com.deepdrilling.jei.DrillJEIPlugin"
    ]
  },
  "mixins": [
    "deepdrilling.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}"
  }
}